# Server Configuration
PORT=3000
NODE_ENV=production

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Proxy Configuration (Primary - Thordata)
PROXY_STRING=

# Additional Proxies (optional, comma-separated format: host:port:user:pass)
# ADDITIONAL_PROXIES=proxy1.com:8080:user:pass,proxy2.com:8080:user:pass

# Rate Limiting Configuration
MAX_CONCURRENT=3
MIN_TIME=500

# Caching Configuration
CACHE_TTL_SECONDS=3600

# Browserless Configuration
BROWSERLESS_ENABLED=true
BROWSERLESS_URL=ws://localhost:3301